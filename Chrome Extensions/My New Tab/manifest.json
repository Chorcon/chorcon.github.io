{
  "manifest_version": 3,
  "name": "My New Tab",
  "version": "1.0.0",
  "description": "A simple newTab with my own photos and layout",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "sidePanel"],
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}
